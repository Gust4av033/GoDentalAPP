<Window x:Class="GoDentalAPP.MainWindow"
       xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
       xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
       xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
       xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
       xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes"
       xmlns:local="clr-namespace:GoDentalAPP"
       mc:Ignorable="d"
       Title="MainWindow" Height="450" Width="800">
   <Grid>
       <StackPanel VerticalAlignment="Center" HorizontalAlignment="Center">
           <TextBox Width="300" materialDesign:HintAssist.Hint="Usuario" />
           <TextBox Width="300" materialDesign:HintAssist.Hint="Contraseña" />
           <Button Content="Iniciar Sesión" Width="300" />
       </StackPanel>
   </Grid>
</Window>
