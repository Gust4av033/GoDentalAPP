<Window x:Class="GoDentalAPP.src.GoDentalAPP.APP.Views.ViewsProducto.ProductosMainWindow"
        Style="{StaticResource {x:Type Window}}"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GoDentalAPP.src.GoDentalAPP.APP.Views.ViewsProducto"
        mc:Ignorable="d"
        Title="ProductosMainWindow" Height="450" Width="800">
    <Grid>
        <DataGrid ItemsSource="{Binding Productos}" 
                  AutoGenerateColumns="True"
                  CanUserAddRows="False"
                  IsReadOnly="True"
                  Margin="10"/>
    </Grid>
</Window>
